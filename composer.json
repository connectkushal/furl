{
    "name": "connectkushal/furl",
    "description": "A library to write fluent curl in php, with laravel framework integration out of the box",
    "type": "library",
    "require": {
        "symfony/process": "^4.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kushal Billaiya",
            "email": "kushal.billaiya@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Connectkushal\\Furl": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Connectkushal\\Furl\\Laravel\\FurlServiceProvider"
            ],
            "aliases": {
                "Furl": "Connectkushal\\Furl\\Laravel\\FurlFacade"
            }
        }
    }
}
